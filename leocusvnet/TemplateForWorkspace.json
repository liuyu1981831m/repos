						"source": [
							"%%pyspark\r\n",
							"import requests\r\n",
							"import json\r\n",
							"\r\n",
							"sessionTokenJObj = json.loads(sessionToken)\r\n",
							"tokenServiceUrl = sessionTokenJObj[\"tokenServiceEndpoint\"]\r\n",
							"fullUrl = \"{0}/api/v1/proxy/subscriptions/4e1adcf4-0906-4efe-bd5d-3659241ab819/resourceGroups/parask/providers/Microsoft.Synapse/workspaces/paraskbrazilsouth/hms/databases/DB1/fetch?api-version=2019-06-01-preview\".format(tokenServiceUrl)\r\n",
							"headers = {\r\n",
							"    'x-ms-proxy-host': 'https://rpbrazilsouth.svc.datafactory.azure.com:44433', # SyMS endpoint\r\n",
							"    'x-ms-cluster-identifier': clusterId,\r\n",
							"    'Authorization': sessionTokenJObj['sessionToken'],\r\n",
							"    'Metadata': 'true'\r\n",
							"}\r\n",
							"\r\n",
							"payload = {'ArtifactName': 'DB1', 'HmsRuntimeProperties': { 'SparkSessionToken': sessionTokenJObj['sessionToken'] } }\r\n",
							"response = requests.post(fullUrl, json=payload, headers=headers)\r\n",
							"\r\n",
							"print(response)\r\n",
							"print(response.text)"
						],
